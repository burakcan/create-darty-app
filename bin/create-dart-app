#!/usr/bin/env node

'use strict';

process.on('unhandledRejection', (err) => {
    throw err;
});

const args = process.argv.slice(2);

const commandFunc = require('./scripts/create');

if (commandFunc.error !== null) {
    console.error(commandFunc.error);

    process.exit(1);
    return;
}

commandFunc.func(...commandFunc.args);
